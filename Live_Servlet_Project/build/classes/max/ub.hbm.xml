<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class name="dto.RegisDTO" table="regis_detail" >
	
		<id name="uid" column="uid">
			<generator class="assigned" />
		</id>	
		
		<property name="uname"      length= "20"  column="uname"   />	
		<property name="add"    length= "20"  column="add" />	
		<property name="adhar"      length= "20"  column="adhar"   />	
		<property name="dob"    length= "20"  column="dob" />	
		<property name="gender"      length= "20"  column="gndr"   />	
		<property name="skill"    length= "20"  column="skill" />	
		<property name="education"      length= "20"  column="education"   />	
			
			
	</class>
	
	
	



	
	
</hibernate-mapping>